{
  "name": "formationsal",
  "version": "0.5.0",
  "private": true,
  "homepage": "./",
  "license": "MIT",
  "description": "Azur Lane formation utility application",
  "main": "./dist/main.bundle.js",
  "author": "RuTle",
  "email": "rutle4@gmail.com",
  "keywords": [
    "ReactJS",
    "Electron",
    "Typescript"
  ],
  "build": {
    "productName": "FormationsAL",
    "appId": "com.FormAL",
    "extraResources": [
      {
        "from": "src/data/ships.json",
        "to": "."
      }
    ],
    "directories": {
      "app": "./dist/",
      "output": "./out/"
    },
    "mac": {
      "target": "dmg"
    },
    "win": {
      "target": "nsis",
      "asar": true
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "buildVersion": "1"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.29",
    "@fortawesome/free-solid-svg-icons": "^5.13.1",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "@reduxjs/toolkit": "^1.5.0",
    "@types/react-redux": "^7.1.15",
    "@types/react-router": "^5.1.11",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-virtualized-auto-sizer": "^1.0.0",
    "@types/react-window": "^1.8.2",
    "electron-is-dev": "^1.2.0",
    "electron-store": "^6.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-modal": "^3.12.1",
    "react-overlays": "^4.1.1",
    "react-redux": "^7.2.2",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "react-tooltip": "^4.2.13",
    "react-virtualized-auto-sizer": "^1.0.3",
    "react-window": "^1.8.6",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "start": "electron ./dist/main.bundle.js",
    "start-test": "nodemon --exec electron ./dist/main.bundle.js",
    "start-test-test": "nodemon --watch dist --exec electron ./dist/main.bundle.js",
    "dev": "rimraf dist && cross-env NODE_ENV=development webpack --watch --progress --color",
    "prod": "rimraf dist && cross-env NODE_ENV=production webpack --progress --color",
    "test": "mocha -r ts-node/register -r tsconfig-paths/register \"test/**/*.ts\"",
    "build:win": "electron-builder build --win --x64",
    "build:mac": "electron-builder build --mac --x64"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react": "^16.14.2",
    "@types/react-dom": "^16.9.10",
    "@types/react-modal": "^3.10.6",
    "@typescript-eslint/eslint-plugin": "^3.5.0",
    "@typescript-eslint/parser": "^3.5.0",
    "concurrently": "^5.3.0",
    "copy-webpack-plugin": "^7.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "electron": "^11.1.1",
    "electron-builder": "^22.9.1",
    "eslint": "^7.3.1",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-webpack": "^0.13.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.5",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.1",
    "lodash": "^4.17.20",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "source-map-loader": "^2.0.0",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.14",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.7",
    "wait-on": "^5.0.1",
    "webpack": "^5.12.0",
    "webpack-cli": "^4.3.1"
  }
}
