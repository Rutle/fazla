{
  "name": "formationsal",
  "version": "0.1.0",
  "private": true,
  "homepage": "./",
  "license": "MIT",
  "description": "Azur Lane formation utility application",
  "main": "./electron/electron.js",
  "author": "RuTle",
  "email": "rutle4@gmail.com",
  "keywords": [
    "ReactJS",
    "Electron",
    "Typescript"
  ],
  "build": {
    "productName": "FormationsAL",
    "appId": "com.FormAL",
    "extends": null,
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "electron/electron.js"
    ],
    "extraResources": [
      {
        "from": "src/data/ships.json",
        "to": "."
      }
    ],
    "directories": {
      "output": "release"
    }
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.29",
    "@fortawesome/free-solid-svg-icons": "^5.13.1",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "@reduxjs/toolkit": "^1.5.0",
    "@types/react-redux": "^7.1.15",
    "@types/react-router": "^5.1.10",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-virtualized-auto-sizer": "^1.0.0",
    "@types/react-window": "^1.8.2",
    "electron-is-dev": "^1.2.0",
    "electron-reload": "^1.5.0",
    "electron-store": "^6.0.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-modal": "^3.12.1",
    "react-overlays": "^4.1.1",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "react-tooltip": "^4.2.11",
    "react-virtualized-auto-sizer": "^1.0.3",
    "react-window": "^1.8.6",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "electron": "concurrently \"tsc ./electron/electron.ts -w\" \"cross-env NODE_ENV=development nodemon --exec \"\"wait-on http://localhost:3000 && electron electron/electron.js\"\"",
    "start": "cross-env NODE_ENV=development BROWSER=none npm run react-start",
    "react-start": "react-scripts start",
    "react-build": "react-scripts build",
    "react-test": "react-scripts test",
    "react-eject": "react-scripts eject",
    "preelectron-build": "npm run react-build",
    "electron-pack": "electron-builder build --win",
    "build": "npm run react-build && npm run electron-build",
    "electron-start": "electron electron/electron.js",
    "ele-dev-test": "SET NODE_ENV=development&& npm run electron-start"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react": "^16.14.2",
    "@types/react-dom": "^16.9.10",
    "@types/react-modal": "^3.10.6",
    "@typescript-eslint/eslint-plugin": "^3.5.0",
    "@typescript-eslint/parser": "^3.5.0",
    "concurrently": "^5.2.0",
    "cross-env": "^7.0.2",
    "electron": "^9.1.2",
    "electron-builder": "^22.7.0",
    "eslint": "^7.3.1",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.5",
    "nodemon": "^2.0.7",
    "prettier": "^2.0.5",
    "typescript": "^3.9.7",
    "wait-on": "^5.0.1"
  }
}
