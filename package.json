{
  "name": "fazla",
  "version": "0.7.0",
  "private": true,
  "homepage": "http://Rutle.github.io/fazla",
  "license": "MIT",
  "description": "Formation utility tool for Azur Lane.",
  "main": "./dist/main.bundle.js",
  "author": "Jussi RistimÃ¤ki",
  "email": "rutle4@gmail.com",
  "keywords": [
    "ReactJS",
    "Electron",
    "Typescript"
  ],
  "build": {
    "productName": "Fazla",
    "appId": "com.Fazla",
    "extends": null,
    "directories": {
      "app": "./dist/",
      "output": "./out/"
    },
    "mac": {
      "target": "dmg"
    },
    "win": {
      "target": "nsis",
      "asar": true
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "buildVersion": "1"
  },
  "dependencies": {
    "@fontsource/open-sans": "^4.2.1",
    "@reduxjs/toolkit": "^1.5.0",
    "@types/node": "^14.14.31",
    "electron-store": "^7.0.2",
    "localforage": "^1.9.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-modal": "^3.12.1",
    "react-overlays": "^4.1.1",
    "react-redux": "^7.2.2",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-virtualized-auto-sizer": "^1.0.4",
    "react-window": "^1.8.6",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "start": "electron ./dist/main.bundle.js",
    "dev": "rimraf dist && cross-env NODE_ENV=development PLAT_ENV=electron webpack --watch --progress --color",
    "prod": "rimraf dist && cross-env NODE_ENV=production PLAT_ENV=electron webpack --progress --color",
    "web-dev": "rimraf dist && cross-env NODE_ENV=development PLAT_ENV=web webpack --watch --progress --color",
    "web-prod": "rimraf dist && cross-env NODE_ENV=production PLAT_ENV=web webpack --progress --color",
    "gh-deploy": "gh-pages -d dist",
    "dev:analyze": "npm run web-dev --env.addons=bundleanalyzer",
    "prod:analyze": "npm run web-prod --env.addons=bundleanalyzer",
    "build:win": "electron-builder build --win --x64",
    "build:mac": "electron-builder build --mac --x64",
    "serve": "cross-env NODE_ENV=development PLAT_ENV=web webpack serve --open"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/react-modal": "^3.12.0",
    "@types/react-redux": "^7.1.16",
    "@types/react-router": "^5.1.11",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-transition-group": "^4.4.1",
    "@types/react-virtualized-auto-sizer": "^1.0.0",
    "@types/react-window": "^1.8.2",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "copy-webpack-plugin": "^7.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.2",
    "electron": "^11.3.0",
    "electron-builder": "^22.9.1",
    "eslint": "^7.20.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-webpack": "^0.13.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "gh-pages": "^3.1.0",
    "html-webpack-plugin": "^5.2.0",
    "lodash": "^4.17.21",
    "mini-css-extract-plugin": "^1.3.8",
    "prettier": "^2.2.1",
    "react-transition-group": "^4.4.1",
    "rimraf": "^3.0.2",
    "source-map-loader": "^2.0.1",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.17",
    "typescript": "^4.1.5",
    "url-loader": "^4.1.1",
    "webpack": "^5.23.0",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  }
}
